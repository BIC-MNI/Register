
CC           = cc -fullwarn -n32 -mips4 -woff 1174
LINK         = cc -n32 -mips4
LINT         = lint -mux
MAKE_LIBRARY = ar r
RANLIB       = @/bin/true
OPT_g        = -g
OPT_O        = -O 
IRIS_GL_SYS_LIBS = -lfm -lgl

.SUFFIXES: .ln .i .u

.c.o:
	$(CC) -c $(CFLAGS) $< -o $@

.c.u:
	$(CC) $(CFLAGS) -O3 -j $< -o $@

.c.i:
	$(CC) -P $(CFLAGS) -c $< -o $@

.c.ln:
	$(LINT) $(LINTFLAGS) -c $<
	@if( "`echo $@ | sed -e 's/.*\///'`" != "$@" ) \
            mv `echo $@ | sed -e 's/.*\///'` $@
